	public static void main(String[] args) throws NamingException {
		InitialContext context = new   InitialContext();

		FirstEjb firstEjb =(FirstEjb)context.lookup("FirstEjbBea/remote");
		firstEjb.sayHello("shadow silent");
		
	}
}
